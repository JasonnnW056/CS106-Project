<UserControl x:Class="CS106_Project.Views.UserControls._404NotFound"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CS106_Project.Views.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel Grid.Row="1" Background="White" Height="640">
            <Image Source="/Images/NotFoundIcon.jpg" Width="400" Margin="0 90 0 0"/>
            <TextBlock Text="404 Not Found!" Style="{StaticResource HeroTitleStyle}" Margin="0 60 0 0" Foreground="{StaticResource MedicalCrimson}"/>
        </StackPanel>
    </Grid>
</UserControl>
